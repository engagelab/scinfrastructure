# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
#GET     				/addUser								controllers.SUsers.addUser


# Group Services
GET     				/group									controllers.SGroups.fetchAllGroups
POST     				/group									controllers.SGroups.createGroup

GET     				/group/:runId							controllers.SGroups.fetchGroupsByRunId(runId:String)

GET     				/group/member/:groupId					controllers.SGroups.fetchGroupMembers(groupId:String)
POST     				/group/member							controllers.SGroups.addMember


#Fake Calls
#GET     				/fgroup									controllers.SGroups.createGroup






























# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
