# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
#GET     				/addUser								controllers.SUsers.addUser


# Group Services
GET     				/group									controllers.SGroups.fetchAllGroups
POST     				/group									controllers.SGroups.createGroup

GET     				/group/member/:groupId								controllers.SGroups.addMember(groupId:String)


#Fake Calls
GET     				/fgroup									controllers.SGroups.createGroup






























# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
